pipeline{

agent any

tools {
    maven "maven3.9.2"
}

stages{
    stage('clonefromsourcecode'){
        steps{
            sh "echo 'clone latest code from github'"
            sh "echo 'build for dev environment june30'"
           git 'https://github.com/Lion-Technology-Solutions/liontech-emr-health-app.git'

        }
    }
    stage('test+build+package')
        steps{
            sh "running sonarqube code analysis"
            sh "running third pipeline project"
            sh "testing must passed in order to proceed"
            sh "mvn install"
            sh "mvn validate"
            sh "mvn test"
            sh "mvn clean package"


        }


} 










}